<div class="row">
	<h2 style="text-align:center;"><span class="fa fa-bar-chart"></span> Lista de dispositivos:</h2>
	<br>
	<div ng-controller="devicesCtrl">
		<table st-table="rowCollection" class="table table-striped">
			<thead>
			<tr>
				<th>first name</th>
				<th>last name</th>
				<th>birth date</th>
				<th>balance</th>
				<th>email</th>
			</tr>
			</thead>
			<tbody>
			<tr ng-repeat="row in rowCollection">
				<td>{{row.firstName}}</td>
				<td>{{row.lastName}}</td>
				<td>{{row.birthDate}}</td>
				<td>{{row.balance}}</td>
				<td>{{row.email}}</td>
			</tr>
			</tbody>
		</table>
	</div>
</div>
