<!doctype html>
<html lang="en" ng-app="superhero">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/png" href="img/favicon.jpg">

    <title>HomeStark Interface</title>
    <!-- STYLES -->
    <!-- build:css lib/css/components.min.css -->
    <link rel="stylesheet" type="text/css" href="components/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="components/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="components/parsleyjs/src/parsley.css"/>
    <link rel="stylesheet" type="text/css" href="components/semantic/dist/semantic.min.css"/>
    <link rel="stylesheet" type="text/css" href="components/angular-video-background/src/video-background.css"/>
    <link rel="stylesheet" type="text/css" href="css/style.css"/>
    <!-- endbuild -->

    <!-- SCRIPTS -->
    <!-- build:js lib/js/components.min.js -->
    <script type="text/javascript" src="components/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="components/angular/angular.min.js"></script>
    <script type="text/javascript" src="components/angular-bootstrap/ui-bootstrap.min.js"></script>
    <script type="text/javascript" src="components/angular-route/angular-route.js"></script>
    <script type="text/javascript" src="custom-components/custom-js/parsley.js"></script>
    <script type="text/javascript" src="components/semantic/dist/semantic.min.js"></script>
    <script type="text/javascript" src="components/vide/dist/jquery.vide.min.js"></script>
    <!-- endbuild -->

    <!-- Custom Scripts -->
    <script type="text/javascript" src="js/homestark.min.js"></script>

    <script type="text/javascript">
    $(document).ready(function(){
      $(".push_menu").click(function(){
           $(".wrapper").toggleClass("active");
      });
      $('.menu li a').click(function(e) {
          e.preventDefault();
          $('a').removeClass('active');
          $(this).addClass('active');
      });
    });
    </script>
</head>
<body>
	<nav class="navbar navbar-default navbar-fixed-top home_style" role="navigation">
    <div class="container">
      <div class="navbar-header">
        <a href="#"><img class="pull-left" src="img/logo_symbol.png" width="5%" heigth="auto" style="margin-top:1%;" alt="HomeStark"/></a>
        <a href="#" id="ntext" style="margin-left:1%; font-size:large; font-style?" class="navbar-text logo_homestark">HomeStark</a>
      </div>
      <span class="navbar-text navbar-right"><a href="#" style="text-decoration: none; margin-right: 10px;" class="logo_homestark"><i class="fa fa-sign-out" aria-hidden="true"></i>Logout</a></span>
    </div>
	</nav>

  <div class="container container_left">
  	<div class="row">
  		<div class="wrapper">
	      <div class="side-bar">
          <ul>
            <li class="menu-head">
                Menu <a  href="#" class="push_menu"><span class="glyphicon glyphicon-align-justify pull-right"></span></a>
            </li>
            <div class="menu">
                <li >
                    <a id="link_1" class="active"  href="#">Dashboard<span class="glyphicon glyphicon-cloud pull-right"></span></a>
                </li>
                <li >
                    <a id="link_2" class="" href="#">Dispositivos<span class="glyphicon glyphicon-tasks pull-right"></span></a>
                </li>
                <li >
                    <a id="link_3" class="" href="#/about">Configurações <span class="glyphicon glyphicon-cog pull-right"></span></a>
                </li>
            </div>
          </ul>
  	    </div>
  		</div>
  	</div>
  </div>

  <div id="page-content-wrapper">
    <div class="container-fluid">
        <!-- Main Content -->
        <div id="mainWindow" class="row">
          <div ng-view></div>
        </div>
    </div>
  </div>
